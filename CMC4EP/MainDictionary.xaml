                        <Trigger Property="IsSelected" Value="False">
                            <Setter TargetName="Map_path" Property="Fill" Value="{StaticResource Tab_normal}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--Стиль Таб Контрола-->
    <Style x:Key="Tab_style" TargetType="TabControl">
        <Setter Property = "TabStripPlacement" Value = "Left"/>
        <Setter Property = "Foreground" Value= "Blue"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TabControl">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="88"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="480"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="105"/>
                                    <RowDefinition Height="385"/>
                                </Grid.RowDefinitions>
                                <Grid.Background>
                                    <VisualBrush>
                                        <VisualBrush.Visual>
                                            <Canvas HorizontalAlignment="Left" Height="480" UseLayoutRounding="False" VerticalAlignment="Top" Width="88.211" Margin="0,0,0,-159.789">
                                                <Canvas Height="105" Width="88">
                                                    <Path Data="F1M88.211,88.211L44.106,107.25 0,88.211 0,0 88.211,0z" Fill="#FF2D4165" Height="107.25" Width="88.211"/>
                                                </Canvas>
                                                <Canvas Height="382" Width="88" Canvas.Left="0.211">
                                                    <Path Data="F1M88.211,0L44.106,19.039 0,0 0,384.789 88.211,384.789z" Fill="#FF2D4165" Height="384.789" Width="88.211" Canvas.Top="95.001"/>
                                                </Canvas>
                                            </Canvas>
                                        </VisualBrush.Visual>
                                    </VisualBrush>
                                </Grid.Background>
                                <Button Grid.Row="0" x:Name="Play_button" Click="Play_button_Click" Style="{StaticResource play}" Height="105" Width="88" Margin="0,0,0,0" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
                                <TabPanel 
                                    Grid.Row="1"
                                    VerticalAlignment="Top"
                                    Margin="0,20,0,0"
                                    Name="HeaderPanel"
                                    Panel.ZIndex="1" 
                                    IsItemsHost="True"
                                    KeyboardNavigation.TabIndex="1"
                                    HorizontalAlignment="Center"/>
                            </Grid>
                            <Grid Grid.Row="1" Background="{StaticResource c_Tab_backround}"/>
                        </Grid>
                        <Grid Grid.Column="2">
                            <Border x:Name="Border"
                                      KeyboardNavigation.TabNavigation="Local"
                                      KeyboardNavigation.DirectionalNavigation="Contained"
                                      KeyboardNavigation.TabIndex="2"
                                    Background="{StaticResource c_Tab_backround}">
                                <ContentPresenter 
                                    x:Name="PART_SelectedContentHost"
                                    Margin="4"
                                    ContentSource="SelectedContent"/>
                            </Border>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--                              ВКЛАДКА ЧАТА                               -->
    
    <!--Стиль Таба-->
    <Style x:Key="Chat_tab" TargetType="TabControl">
        <Setter Property = "TabStripPlacement" Value = "Top"/>
        <Setter Property = "Foreground" Value= "Blue"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TabControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="32"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="5"/>
                            <RowDefinition Height="32"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0">
                            <TabPanel 
                                    Grid.Row="1"
                                    VerticalAlignment="Bottom"
                                    HorizontalAlignment="Left"
                                    Name="HeaderPanel"
                                    Panel.ZIndex="1" 
                                    IsItemsHost="True"
                                    KeyboardNavigation.TabIndex="1"
                                    />
                        </Grid>
                        <Grid Grid.Row="1">
                            <Border x:Name="Border"
                                      KeyboardNavigation.TabNavigation="Local"
                                      KeyboardNavigation.DirectionalNavigation="Contained"
                                      KeyboardNavigation.TabIndex="2"
                                    Background="{StaticResource chat_Tab_backround}">
                                <ContentPresenter 
                                    x:Name="PART_SelectedContentHost"
                                    Margin="4"
                                    ContentSource="SelectedContent"/>
                            </Border>
                        </Grid>
                        <Grid Grid.Row="3">
                            <Border>
                                <Grid Background="{DynamicResource chat_Tab_backround2}" Height="32" MinWidth="{DynamicResource i_search_min_width}">

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="40"/>
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Column="0">
                                        <TextBox HorizontalAlignment="Stretch"                      
                                                 VerticalAlignment="Center"
                                                 Height="32" 
                                                 TextWrapping="Wrap" 
                                                 Text="Поиск"  
                                                 Background="{DynamicResource chat_Tab_backround2}"
                                                 VerticalContentAlignment="Center"
                                                 HorizontalContentAlignment="Left" 
                                                 FontSize="14" 
                                                 FontFamily="{DynamicResource Default}"
                                                 BorderThickness="0"/>
                                    </Grid>
                                    <Grid Grid.Column="1">
                                        <Grid.Background>
                                            <VisualBrush>
                                                <VisualBrush.Visual>
                                                    <Path Fill="{StaticResource btn_background}" Data="F1M6.925,32.087L0,16.044 6.925,0 39.012,0 39.012,32.087z" Height="32.087" Canvas.Left="0" Canvas.Top="0" Width="39.012"/>
                                                </VisualBrush.Visual>
                                            </VisualBrush>
                                        </Grid.Background>
                                        <Ctrl:ChatButton Height="19" Width="17" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,1,-4,0"/>
                                    </Grid>
                                </Grid>
                            </Border>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--Стиль табитема Global-->
    <Style x:Key="GlobalChat" TargetType="TabItem">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TabItem">
                    <Border Background="{StaticResource chat_Tab_backround}">
                        <Border Width="52" Height="27"  Name="Border" BorderBrush="{StaticResource chat_Tab_backround}" BorderThickness="2">
                            <ContentPresenter x:Name="ContentSite"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"/>
                        </Border>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource chat_Tab_backround2}" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="False">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource chat_Tab_backround}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
