<UserControl x:Class="Ctrl.ChatButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ctrl"
             mc:Ignorable="d" 
             d:DesignHeight="19" d:DesignWidth="17">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <Button x:Name="Chat_button" Click="Chat_button_Click"  Style="{StaticResource chat_btn}" Height="19" Width="17" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
