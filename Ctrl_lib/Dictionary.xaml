    <!--Фон нажатой кнопки-->
    <VisualBrush x:Key="SLB_hover">
        <VisualBrush.Visual>
            <Canvas Height="32" Width="40">
                <Path Fill="{StaticResource btn_background}" Data="F1M6.925,32.087L0,16.044 6.925,0 39.012,0 39.012,32.087z" Height="32.087" Width="39.012"/>
                <Path Fill="{StaticResource btn_ligth}" Data="F1M2.024,1.748C-0.564,4.184 -0.687,8.256 1.75,10.845 3.947,13.18 7.481,13.506 10.051,11.755L11.231,13.008C11.126,13.529,11.265,14.09,11.657,14.507L17.561,20.778C18.201,21.458,19.272,21.491,19.951,20.85L20.008,20.795C20.689,20.156,20.721,19.084,20.081,18.405L14.177,12.134C13.785,11.718,13.234,11.543,12.709,11.618L11.529,10.366C13.432,7.904 13.318,4.359 11.119,2.023 9.853,0.678 8.145,0 6.433,0 4.851,-0.001 3.266,0.579 2.024,1.748 M2.773,9.879C0.869,7.858 0.966,4.676 2.988,2.772 5.01,0.87 8.191,0.965 10.095,2.987 11.999,5.009 11.903,8.191 9.88,10.094 8.909,11.008 7.67,11.461 6.435,11.461 5.097,11.461 3.762,10.93 2.773,9.879" Height="21.31" Canvas.Left="10.711" Canvas.Top="5.389" Width="20.541"/>
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>
    
    <!--Стиль кнопки поиска-->
    <Style x:Key="search_list_button" TargetType="Button">
        <Setter Property="SnapsToDevicePixels" Value="true"/>
        <Setter Property="OverridesDefaultStyle" Value="true"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border 
                                      x:Name="Border"  
                                      Background="{StaticResource SLB_normal}">
                        <ContentPresenter 
                                                        Margin="2"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        RecognizesAccessKey="True"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource SLB_hover}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource SLB_hover}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--////////////////////////////////////////       СТРОКА ПОИСКА (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////       КНОПКА ВЫХОДА (начало)          //////////////////////////////////////////////////-->

    <!-- Фон пассивной кнопки -->
    <VisualBrush x:Key="ExitNormalBrush">
        <VisualBrush.Visual>
            <Canvas Width="3780" Height="3780" Clip="F1 M 0,3779.53L 3779.53,3779.53L 3779.53,0L 0,0L 0,3779.53 Z ">
                <Path Fill="{StaticResource btn_fill}" Width="46.1947" Height="46.1947" Canvas.Left="1794.52" Canvas.Top="1704" Stretch="Fill" Data="F1 M 1840.71,1743.19L 1833.71,1750.19L 1794.52,1711L 1801.52,1704L 1840.71,1743.19 Z "/>
                <Path Fill="{StaticResource btn_fill}" Width="46.1946" Height="46.1947" Canvas.Left="1794.52" Canvas.Top="1704" Stretch="Fill" Data="F1 M 1801.52,1750.19L 1794.52,1743.19L 1833.71,1704L 1840.72,1711L 1801.52,1750.19 Z "/>
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>

    <!-- Фон активной кнопки -->
    <VisualBrush x:Key="ExitPressedBrush">
        <VisualBrush.Visual>
            <Canvas Width="46.6667" Height="46.6667" Canvas.Left="0" Canvas.Top="0">
                <Path Fill="{StaticResource btn_ligth}" Width="46.1934" Height="46.1934" Canvas.Left="0" Canvas.Top="0.473309" Stretch="Fill" Data="F1 M 46.1934,39.664L 39.1907,46.6667L 0,7.47469L 7.00269,0.473309L 46.1934,39.664 Z "/>
                <Path Fill="{StaticResource btn_ligth}" Width="46.1934" Height="46.1947" Canvas.Left="0" Canvas.Top="0.472008" Stretch="Fill" Data="F1 M 7.00269,46.6667L 0,39.664L 39.192,0.472008L 46.1934,7.47469L 7.00269,46.6667 Z "/>
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>
    
    <!--Стиль кнопки выхода-->
    <Style x:Key="Exit_button" TargetType="Button">
        <Setter Property="SnapsToDevicePixels" Value="true"/>
        <Setter Property="OverridesDefaultStyle" Value="False"/>
        <Setter Property="MinHeight" Value="30"/>
        <Setter Property="MinWidth" Value="30"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border 
                                      x:Name="Border"  
                                      CornerRadius="2" 
                                      BorderThickness="1"
                                      Background="{StaticResource ExitNormalBrush}">
                        <ContentPresenter 
                                                        Margin="2"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        RecognizesAccessKey="True"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource ExitPressedBrush}" />
                        </Trigger>
                        <!--Цвет при нажатии мыши -->
                        <Trigger Property="IsPressed" Value="true">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource ExitPressedBrush}" />
                            <Setter TargetName="Border" Property="Margin" Value="2"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--////////////////////////////////////////       КНОПКА ВЫХОДА (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////       ПОПАП С ФИЛЬТРАМИ (начало)          //////////////////////////////////////////////////-->

    <VisualBrush x:Key="Admin_chk_on">
        <VisualBrush.Visual>
