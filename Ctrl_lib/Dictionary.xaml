<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    x:Class="Ctrl.Dictionary">

    <!--////////////////////////////////////////       Содержание (начало)          //////////////////////////////////////////////////-->

    <!--
    1. Шрифты
    2. Цвета
    3. Стили списка поняш
    4. Строка поиска
    5. Кнопка выхода
    6. Попап с фильтрами
    7. Userinform
    8. Все для окна авторизации
    9. Все для вкладки чата
    -->

    <!--////////////////////////////////////////       Содержание (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////       ШРИФТЫ (начало)          //////////////////////////////////////////////////-->

    <FontFamily x:Key="Default">
        <FontFamily.FamilyNames>
            <sys:String x:Key="en-US">Arial</sys:String>
        </FontFamily.FamilyNames>            
    </FontFamily>

    <!--////////////////////////////////////////       ШРИФТЫ (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////       ЦВЕТА (начало)          //////////////////////////////////////////////////-->

    <!--Самый темный-->
    <Color x:Key="c_Dark">#2e4165</Color>
    <!--Темный-->
    <Color x:Key="c_Normal">#6792dd</Color>
    <!--Светлый-->
    <Color x:Key="c_Light">#a0bdf0</Color>
    
    <!--Цвета пони-->

    <!--earthpony online-->
    <SolidColorBrush x:Key="pc_earthpony_online" x:Name="ponycolor" Color="#FF82C341"/>
    <!--earthpony offline-->
    <SolidColorBrush x:Key="pc_earthpony_offline" Color="#B382C341"/>
    <!--pegasus online-->
    <SolidColorBrush x:Key="pc_pegasus_online" Color="#FF83D2E0"/>
    <!--pegasus offline-->
    <SolidColorBrush x:Key="pc_pegasus_offline" Color="#B383D2E0"/>
    <!--unihorn online-->
    <SolidColorBrush x:Key="pc_unihorn_online" Color="#FFF15758"/>
    <!--unihorn offline-->
    <SolidColorBrush x:Key="pc_unihorn_offline" Color="#B3F15758"/>
    <!--zebra online-->
    <SolidColorBrush x:Key="pc_zebra_online" Color="#FF5760AC"/>
    <!--zebra offline-->
    <SolidColorBrush x:Key="pc_zebra_offline" Color="#B35760AC"/>
    <!--regular online-->
    <SolidColorBrush x:Key="pc_regular_online" Color="#FFFFFFFF"/>
    <!--regular offline-->
    <SolidColorBrush x:Key="pc_regular_offline" Color="#B3FFFFFF"/>
    <!--moder online-->
    <SolidColorBrush x:Key="moder_online" Color="#FFA3BCE2"/>
    <!--moder offline-->
    <SolidColorBrush x:Key="moder_offline" Color="#B3A3BCE2"/>

    <!--Цвета листа пользователей-->

    <!--Цвет бэка статуса-->
    <SolidColorBrush x:Key="c_Status_backround" Color="{StaticResource c_Dark}"/>
    <!--Цвет фона итемов в листе-->
    <SolidColorBrush x:Key="c_ListItem_backround" Color="{StaticResource c_Light}"/>
    <!--Цвет бордера итема-->
    <SolidColorBrush x:Key="c_ListItem_border" Color="Black"/>
    <!--Цвет фона листа-->
    <SolidColorBrush x:Key="c_ListBox_background" Color="{StaticResource c_Normal}"/>
    <!--Цвет фона скролла-->
    <SolidColorBrush x:Key="c_Scroll_background" Color="{StaticResource c_Normal}"/>
    <!--Цвет бордера списка-->
    <SolidColorBrush x:Key="c_Scroll_border" Color="{StaticResource c_Light}"/>
    
    <!--Цвета строки поиска-->
    
    <!--Цвет фона строки-->
    <SolidColorBrush x:Key="c_search_background" Color="{StaticResource c_Light}"/>
    <!--Цвет бордера строки-->
    <SolidColorBrush x:Key="c_search_border" Color="{StaticResource c_Light}"/>
    
    <!--Цвета попапа-->
    
    <!--Цвет фона попапа-->
    <SolidColorBrush x:Key="c_popup_background" Color="{StaticResource c_Dark}"/>
    <!--Цвет бордера попапа-->
    <SolidColorBrush x:Key="c_popup_border" Color="{StaticResource c_Light}"/>
    <!-- Цвет бордера кнопки F -->
    <SolidColorBrush x:Key="c_F_border" Color="{StaticResource c_Light}"/>
    
    <!--Цвета для UserInform-->
    <!--Цвет фона -->
    <SolidColorBrush x:Key="c_UserInform_background" Color="{StaticResource c_Dark}"/>
    <SolidColorBrush x:Key="c_UserInform_lightBackground" Color="{StaticResource c_Light}"/>

    <!-- Фон ЛистИтема -->
    <SolidColorBrush x:Key="friend_item_background" Color="{StaticResource c_Light}"/>

    <!--Цвета кнопок-->
    
    <!--Стандартная заливка иконок-->
    <SolidColorBrush x:Key="btn_fill" Color="#FFA3BCE2"/>
    <!--Заливка фона-->
    <SolidColorBrush x:Key="btn_background" Color="{StaticResource c_Dark}"/>
    <!--Заливка активных иконок-->
    <LinearGradientBrush x:Key="btn_ligth" EndPoint="0.5,0" StartPoint="0.5,1">
        <GradientStop Color="#FFCAABD1" Offset="0"/>
        <GradientStop Color="#FFA3BCE1" Offset="1"/>
    </LinearGradientBrush>


    <!--////////////////////////////////////////       ЦВЕТА (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////       Размеры (начало)          //////////////////////////////////////////////////-->

    <!--Минимальная ширина списка поняш-->
    <sys:Double x:Key="i_scrollviewer_min_width">266</sys:Double>
    
    <!--Минимальная ширина строки поиска-->
    <sys:Double x:Key="i_search_min_width">266</sys:Double>

    <!--////////////////////////////////////////       Размеры (конец)          //////////////////////////////////////////////////-->

    <!--////////////////////////////////////////        СТИЛИ СПИСКА ПОНЯШ (начало)          //////////////////////////////////////////////////-->

    <!--Суть (тм)
        
        Список сделан из листбокса неограниченной высоты и скроллвьювера, который дает возможность ограничить высоту и прокручивать список
        
        Список имеет свой стиль. Стиль описывает фон, бордеры и прочую мелочь и подключает стиль уже итемов.
        В стиле итемов уже прописаны уже как внешний самих итемов так и контролы(напримеры сзначек нажатия)
        
        Стиль скроллвьювера состоит из стиля области контента (к которой приаттачен список) и стиля скроллбара).
        Стиль скроллбара состоит из набора шаблонов скроллбаров (вертикальный, горизонтальный(в нашем случае только вертикальный)).
        Сами скроллбары состоят из линии прокрутки + стиль кнопок прокруток(не реализовано)-->

    <!--Стиль итемов-->
    <Style x:Key="Pony_list_item" TargetType="{x:Type ListBoxItem}">
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Padding" Value="4,1"/>
        <Setter Property="HorizontalContentAlignment" Value="{Binding HorizontalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
        <Setter Property="VerticalContentAlignment" Value="{Binding VerticalContentAlignment, RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListBoxItem}">
                    <Border Margin="5,5,5,5" BorderThickness="1" BorderBrush="{StaticResource c_ListItem_border}" Width="230" Background="{StaticResource c_ListItem_backround}" HorizontalAlignment="Left">
                        <Grid Margin="0" Height="32">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="25"/>
                                <ColumnDefinition Width="180"/>
                                <ColumnDefinition Width="25"/>
                            </Grid.ColumnDefinitions>

                            <Grid Grid.Column="0" Background="{StaticResource c_Status_backround}">
                                <Viewbox Width="15" Height="15" VerticalAlignment="Center">
                                    <Canvas Height="29.14" Width="29.139">
                                        <Path Data="F1M29.14,14.57C29.14,22.616 22.617,29.14 14.57,29.14 6.523,29.14 0.001,22.616 0.001,14.57 0.001,6.523 6.523,0 14.57,0 22.617,0 29.14,6.523 29.14,14.57" Fill="{Binding statusColor}" Height="29.14" Width="29.139"/>
                                    </Canvas>
                                </Viewbox>
                            </Grid>

                            <Grid Grid.Column="1" Height="32">
                                <Label VerticalContentAlignment="Center" Content="{Binding Name}" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="16" Width="180" Height="32" FontFamily="{StaticResource Default}"/>
                            </Grid>

                            <Grid Grid.Column="2">
                                <Canvas x:Name="selected" Height="18.601" Width="7.625" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Hidden">
                                    <Path Data="F1M-0.001,0L7.624,9.301 -0.001,18.601z" Fill="#2e4165" Height="18.601" Canvas.Left="0" Canvas.Top="0" Width="7.625"/>
                                </Canvas>
                            </Grid>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Selector.IsSelectionActive" Value="False"/>
                                <Condition Property="IsSelected" Value="True"/>
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter Property="Visibility" TargetName="selected" Value="Hidden"/>
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Selector.IsSelectionActive" Value="True"/>
                                <Condition Property="IsSelected" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Visibility" TargetName="selected" Value="Visible"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--Стиль листбокса-->
    <Style x:Key="Pony_list" TargetType="{x:Type ListBox}">
        <Setter Property="ItemContainerStyle" Value="{StaticResource Pony_list_item}"/>
        <Setter Property="Height" Value="auto"/>
        <Setter Property="Width" Value="245"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListBox}">
                    <Border Background="{StaticResource c_ListBox_background}"
                                Height="{Binding Height}"
                                Width="{Binding Width}">
                        <ItemsPresenter/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--Стиль полоски фамба прокрутки-->
    <Style x:Key="Pony_thumb" TargetType="{x:Type Thumb}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Width" Value="13"/>
        <Setter Property="MinHeight" Value="46"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Border Name="Border" 
                                    Width="13">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="13"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="13"/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Ellipse Fill="#FF2D4165" HorizontalAlignment="Left" Height="12" VerticalAlignment="Top" Width="13"/>
                                <Rectangle Fill="#FF2D4165" HorizontalAlignment="Left" Height="7" VerticalAlignment="Bottom" Width="13"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Rectangle Fill="#FF2D4165" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="13" Margin="0"/>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Rectangle Fill="#FF2D4165" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="13" Margin="0"/>
                                <Rectangle Fill="#FF6C91CB" HorizontalAlignment="Center" VerticalAlignment="Center" Width="9" Height="1" Margin="0,9,0,0"/>
                                <Rectangle Fill="#FF6C91CB" HorizontalAlignment="Center" VerticalAlignment="Center" Width="9" Height="1" Margin="0"/>
                                <Rectangle Fill="#FF6C91CB" HorizontalAlignment="Center" VerticalAlignment="Center" Width="9" Height="1" Margin="0,-7,0,0"/>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Rectangle Fill="#FF2D4165" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="13" Margin="0"/>
                            </Grid>
                            <Grid Grid.Row="4">
                                <Ellipse Fill="#FF2D4165" HorizontalAlignment="Left" Height="12" VerticalAlignment="Top" Width="13"/>
                                <Rectangle Fill="#FF2D4165" HorizontalAlignment="Left" Height="6" VerticalAlignment="Top" Width="13"/>
                            </Grid>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--Шаблон вертикального скролера-->
    <ControlTemplate x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
        <Grid Width="21" Height="auto" Background="{StaticResource c_Scroll_background}" HorizontalAlignment="Center">
            <Border>
                <Border Background="#FF2D4165" CornerRadius="3" Width="5" HorizontalAlignment="Center"/>
            </Border>
            <Track      HorizontalAlignment="Center"
                           Name="PART_Track"
                           IsDirectionReversed="true"
                        ViewportSize="NaN">
                <Track.Thumb>
                    <Thumb Style="{StaticResource Pony_thumb}" HorizontalAlignment="Center"/>
                </Track.Thumb>
            </Track>
        </Grid>
    </ControlTemplate>
    <!--Стиль скроллбара-->
    <Style x:Key="ScrollBar" TargetType="{x:Type ScrollBar}">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Style.Triggers>
            <Trigger Property="Orientation" Value="Vertical">
                <Setter Property="Width" Value="21"/>
                <Setter Property="Height" Value="Auto"/>
                <Setter Property="Template" Value="{StaticResource VerticalScrollBar}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <!--Стиль скроллвьювера-->
    <Style x:Key="Pony_scroll" TargetType="{x:Type ScrollViewer}">
        <Setter Property="MinWidth" Value="{StaticResource i_scrollviewer_min_width}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ScrollViewer}">
                    <Border BorderBrush="{StaticResource c_Scroll_border}"
                                BorderThickness="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ScrollContentPresenter 
                                            x:Name="PART_ScrollContentPresenter" 
                                            CanContentScroll="True" 
                                            CanHorizontallyScroll="False" 
                                            CanVerticallyScroll="False" 
                                            ContentTemplate="{TemplateBinding ContentTemplate}" 
                                            Content="{TemplateBinding Content}" 
                                            Grid.Column="0" />
                            <ScrollBar 
                                            x:Name="PART_VerticalScrollBar" 
                                            AutomationProperties.AutomationId="VerticalScrollBar" 
                                            Cursor="Arrow" 
                                            Grid.Column="1" 
                                            Maximum="{TemplateBinding ScrollableHeight}" 
                                            Minimum="0" 
                                            Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}" 
                                            Value="{Binding VerticalOffset, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}"                                 
                                            ViewportSize="{TemplateBinding ViewportHeight}"
                                            Style="{DynamicResource ScrollBar}"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--////////////////////////////////////////        СТИЛИ СПИСКА ПОНЯШ (конец)          //////////////////////////////////////////////////-->


    <!--////////////////////////////////////////       СТРОКА ПОИСКА (начало)          //////////////////////////////////////////////////-->

    <!--Фон кнопки обычный-->
    <VisualBrush x:Key="SLB_normal">
        <VisualBrush.Visual>
            <Canvas Height="32" Width="40">
                <Path Fill="{StaticResource btn_background}" Data="F1M6.925,32.087L0,16.044 6.925,0 39.012,0 39.012,32.087z" Height="32.087" Width="39.012"/>
                <Path Fill="{StaticResource btn_fill}" Data="F1M9.88,10.094C7.859,11.996 4.676,11.901 2.773,9.879 0.87,7.858 0.966,4.676 2.988,2.772 5.01,0.869 8.192,0.965 10.095,2.987 11.999,5.009 11.902,8.191 9.88,10.094 M20.081,18.405L14.177,12.133C13.786,11.717,13.234,11.543,12.709,11.617L11.529,10.365C13.432,7.905 13.318,4.359 11.119,2.023 8.684,-0.564 4.611,-0.687 2.024,1.748 -0.564,4.184 -0.686,8.256 1.75,10.844 3.948,13.18 7.481,13.506 10.051,11.755L11.232,13.008C11.126,13.529,11.266,14.09,11.657,14.506L17.561,20.778C18.201,21.457,19.272,21.49,19.952,20.85L20.008,20.795C20.689,20.156,20.721,19.084,20.081,18.405" Height="21.31" Canvas.Left="10.711" Canvas.Top="5.389" Width="20.541"/>
            </Canvas>
        </VisualBrush.Visual>
