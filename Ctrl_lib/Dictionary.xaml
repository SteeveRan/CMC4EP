                                <Border
                                            x:Name="Border2" 
                                            CornerRadius="2" 
                                            BorderThickness="1"           
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Left"
                                            Width="174"
                                            Height="26">
                                    <ContentPresenter 
                                                RecognizesAccessKey="True" 
                                                VerticalAlignment="Center" 
                                                Margin="2,0,0,0" />
                                </Border>
                            </Grid>
                        </Grid>
                    </Grid>

                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked"  Value="True">
                            <Setter TargetName="Rect_chk" Property="Fill" Value="{StaticResource btn_fill}"/>
                        </Trigger>
                        <Trigger Property="IsChecked"  Value="false">
                            <Setter TargetName="Rect_chk" Property="Fill" Value="{StaticResource btn_background}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

